{
  "name": "CircleCore",
  "version": "1.0.0",
  "description": "CircleCore",
  "main": "index.js",
  "scripts": {
    "lint-fix": "eslint --ext jsx --fix circle_core/web/src/js",
    "clean": "rm -f ./circle_core/web/static/*",
    "build": "npm run clean && run-p build:prod:*",
    "build:prod:js": "webpack --mode production",
    "build:prod:css": "postcss ./circle_core/web/src/css/main.css -o ./circle_core/web/static/main.css",
    "build:dev": "npm run clean && run-p build:dev:*",
    "build:dev:css": "postcss ./circle_core/web/src/css/main.css -o ./circle_core/web/static/main.css",
    "build:dev:js": "webpack --mode development",
    "watch:dev": "npm run clean && run-p watch:dev:*",
    "watch:dev:css": "postcss ./circle_core/web/src/css/main.css -w -o ./circle_core/web/static/main.css",
    "watch:dev:js": "webpack --mode development --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/glucoseinc/CircleCore.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/glucoseinc/CircleCore/issues"
  },
  "browserslist": [
    "last 2 versions",
    "> 5%",
    "not dead"
  ],
  "homepage": "https://github.com/glucoseinc/CircleCore#readme",
  "dependencies": {
    "@testlio/react-title-component": "Testlio/react-title-component#master",
    "immutable": "^4.0.0-rc.12",
    "material-ui": "^0.20.2",
    "moment": "^2.17.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-actions": "^1.2.0",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.1.1",
    "redux-immutable": "^3.0.9",
    "redux-saga": "^0.14.0",
    "rickshaw": "^1.6.0",
    "superagent": "^3.3.1",
    "superagent-prefix": "0.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^1.0.1",
    "eslint": "^5.9.0",
    "eslint-config-google": "^0.11.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^7.11.1",
    "normalizr": "^3.1.0",
    "npm-run-all": "^4.1.3",
    "postcss-cli": "^6.0.1",
    "postcss-import": "^12.0.1",
    "postcss-mixins": "^6.2.0",
    "postcss-preset-env": "^6.4.0",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2"
  }
}
